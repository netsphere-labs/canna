#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = forsort.c

#ifdef __EMX__
AllTarget(forsort.exe)
#else
AllTarget(forsort)
#endif
SingleProgramTarget(forsort,forsort.o,,)
InstallProgram(forsort,$(cannaBinDir))
DependTarget()
LintTarget()
