#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = forcpp.c

#ifdef __EMX__
AllTarget(forcpp.exe)
#else
AllTarget(forcpp)
#endif
SingleProgramTarget(forcpp,forcpp.o,,)
InstallProgram(forcpp,$(cannaBinDir))
DependTarget()
LintTarget()

