#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = ctow.c

#ifdef __EMX__
AllTarget(ctow.exe)
#else
AllTarget(ctow)
#endif
SingleProgramTarget(ctow,ctow.o,,)
InstallProgram(ctow,$(cannaBinDir))
InstallManPage(ctow,$(cannaManDir))
DependTarget()
LintTarget()
