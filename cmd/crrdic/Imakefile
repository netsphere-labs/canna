#include "../../Canna.conf"

      CANNAROOT = ../..
       INCLUDES = -I$(CANNAROOT)/include
           SRCS = crrdic.c

#ifdef __EMX__
AllTarget(crrdic.exe)
#else
AllTarget(crrdic)
#endif
SingleProgramTarget(crrdic,crrdic.o,,)
InstallProgram(crrdic,$(cannaBinDir))
DependTarget()
LintTarget()
