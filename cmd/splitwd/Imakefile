#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = splitwd.c

#ifdef __EMX__
AllTarget(splitword.exe)
#else
AllTarget(splitword)
#endif
SingleProgramTarget(splitword,splitwd.o,,)
InstallProgram(splitword,$(cannaBinDir))
InstallManPageLong(splitwd,$(cannaManDir),splitword)
DependTarget()
LintTarget()
