#include "../../Canna.conf"

      CANNAROOT = ../..
           SRCS = wtoc.c

#ifdef __EMX__
AllTarget(wtoc.exe)
#else
AllTarget(wtoc)
#endif
SingleProgramTarget(wtoc,wtoc.o,,)
InstallProgram(wtoc,$(cannaBinDir))
InstallManPage(wtoc,$(cannaManDir))
DependTarget()
LintTarget()
