#include "../../Canna.conf"

      CANNAROOT = ../..
       CANNASRC = $(CANNAROOT)/lib/canna
       INCLUDES = -I$(CANNAROOT)/include

         TARGET = dpromdic
           OBJS = dpromdic.o
           SRCS = dpromdic.c

#ifdef __EMX__
AllTarget($(TARGET).exe)
#else
AllTarget($(TARGET))
#endif
NormalProgramTarget($(TARGET),$(OBJS),$(DEPCANNALIB),$(CANNALIB),)
InstallProgram($(TARGET),$(cannaBinDir))
InstallManPage($(TARGET),$(cannaManDir))
DependTarget()
LintTarget()
