#include "../../Canna.conf"

    CANNAROOT = ../..
     INCLUDES =
         SRCS = kpdic.c

#ifdef __EMX__
AllTarget(kpdic.exe)
#else
AllTarget(kpdic)
#endif
SingleProgramTarget(kpdic,kpdic.o,,)
InstallProgram(kpdic,$(cannaBinDir))
DependTarget()
LintTarget()

