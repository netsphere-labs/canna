             OBJS = tempdic.o permdic.o dicsw.o ngram.o 			bun.o context.o nword.o ncache.o 			dic.o util.o kana.o 			bits.o dd.o fq.o

              SRCS = dicsw.c permdic.c tempdic.c ngram.c 			context.c bun.c ncache.c 			nword.c dic.c util.c kana.c 			bits.c dd.c fq.c

            HEADER = RK.h
           HEADERS = $(HEADER) RKintern.h

.c.o:
	$(RM) $@
	 $(CC) -c $(CFLAGS) $(_NOOP_) $*.c

all:: libRK.a

libRK.a: $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@
	$(_NULLCMD_)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

lintlib:: llib-lRK.ln

llib-lRK.ln: $(SRCS) $(EXTRALIBRARYDEPS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)RK $(LINTFLAGS) $(SRCS)

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

includes:: $(HEADER)
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	echo "	cd" $(CANNAROOT)/include/canna; cd $(CANNAROOT)/include/canna && \
	for i in $(HEADER); do (set -x; $(RM) $$i; $(LN) ../../lib/RK/$$i .); done

includes:: RKrename.h
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	echo "	cd" $(CANNAROOT)/include; cd $(CANNAROOT)/include && \
	for i in RKrename.h; do (set -x; $(RM) $$i; $(LN) ../lib/RK/$$i .); done

install:: $(HEADER)
	@if [ -d $(DESTDIR)$(cannaIncDir) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(cannaIncDir)); fi
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	for i in $(HEADER); do \
	(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) $$i $(DESTDIR)$(cannaIncDir)); \
	done

