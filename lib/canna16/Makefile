	$(_NULLCMD_)

all:: lib$(LIBNAME).so.$(cannaDsoRev)

lib$(LIBNAME).so.$(cannaDsoRev):  $(OBJS) $(EXTRALIBRARYDEPS)
	$(RM) $@~
	@SONAME=`echo $@ | sed 's/\.[^\.]*$$//'`; set -x; \
	(cd .; $(CC) -o ./$@~ $(SHLIBLDFLAGS) -Wl,-soname,$$SONAME $(OBJS) $(REQUIREDLIBS) -lc); \
	$(RM) $$SONAME; $(LN) $@ $$SONAME; \
	true
	$(RM) $@
	$(MV) $@~ $@
	@if $(SOSYMLINK); then (set -x; \
	$(RM) lib$(LIBNAME).so; \
	$(LN) $@ lib$(LIBNAME).so); fi
	$(_NULLCMD_)
	$(_NULLCMD_)

clean::
	@MAJREV=`expr $(cannaDsoRev) : '\(.*\)\.'`; \
	set -x; $(RM) lib$(LIBNAME).so.$$MAJREV
	$(RM) lib$(LIBNAME).so.$(cannaDsoRev) lib$(LIBNAME).so

install:: lib$(LIBNAME).so.$(cannaDsoRev)
	@if [ -d $(DESTDIR)$(libCannaDir) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(libCannaDir)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTBINFLAGS) lib$(LIBNAME).so.$(cannaDsoRev) $(DESTDIR)$(libCannaDir)
	@T=`echo $(DESTDIR)$(libCannaDir)/lib$(LIBNAME).so.$(cannaDsoRev) | sed 's/\.[^\.]*$$//'`;	set -x; $(RM) $$T && $(LN) lib$(LIBNAME).so.$(cannaDsoRev) $$T
	@if $(SOSYMLINK); then (set -x; $(RM) $(DESTDIR)$(libCannaDir)/lib$(LIBNAME).so; \
	$(LN) lib$(LIBNAME).so.$(cannaDsoRev) $(DESTDIR)$(libCannaDir)/lib$(LIBNAME).so); fi

