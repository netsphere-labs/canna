#!/bin/sh

case "$1" in
    purge)
    rm -rf /var/log/canna
    deluser --remove-home canna
    rm -f /etc/default/canna
    rm -f /etc/hosts.canna
    ;;
    
    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
    ;;
    
    *)
    
    echo "postrm called with unknown argument \`$1'" >&2
    exit 0
    ;;
esac

#DEBHELPER#
