          INCLUDES = -I$(CANNAROOT)/include

            IRSRCS = $(SRCS) $(HEADER)
            HEADER = IR.h IRproto.h IRwproto.h
              SRCS = WaitFor.c connection.c convert.c wconvert.c main.c 			misc.c util.c

              OBJS = WaitFor.o connection.o convert.o wconvert.o main.o 			misc.o util.o

            TARGET = cannaserver
           DEPLIBS = $(LOCAL_LIBRARIES)

all:: $(TARGET)

$(TARGET): $(OBJS) $(DEPLIBS)
	$(RM) $@
	$(CCLINK) -o $@ $(LDOPTIONS) $(OBJS) $(DEPLIBS) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) $(TARGET)

install:: $(TARGET)
	@if [ -d $(DESTDIR)$(cannaSrvDir) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(cannaSrvDir)); fi
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	for i in $(TARGET); do \
	(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTFLAGS) $$i $(DESTDIR)$(cannaSrvDir)); \
	done

server.tmp:: server.man $(MANUALSED)
	sed -f $(MANUALSED) server.man > server.tmp
$(MANUALSED):: $(CANNAROOT)/Canna.conf
	(echo "s%@(cannaBinDir)%$(cannaBinDir)%g"; \
	echo "s%@(cannaLibDir)%$(cannaLibDir)%g"; \
	echo "s%@(cannaIncDir)%$(cannaIncDir)%g"; \
	echo "s%@(ErrDir)%$(ErrDir)%g") > $(MANUALSED)
clean::
	$(RM) $(MANUALSED)

server.jtmp:: server.jmn $(MANUALSED)
	sed -f $(MANUALSED) server.jmn > server.jtmp
$(MANUALSED):: $(CANNAROOT)/Canna.conf
	(echo "s%@(cannaBinDir)%$(cannaBinDir)%g"; \
	echo "s%@(cannaLibDir)%$(cannaLibDir)%g"; \
	echo "s%@(cannaIncDir)%$(cannaIncDir)%g"; \
	echo "s%@(ErrDir)%$(ErrDir)%g") > $(MANUALSED)
clean::
	$(RM) $(MANUALSED)
install.man:: server.tmp server.jtmp
	@if [ -d $(DESTDIR)$(cannaManDir)/man$(MANSUFFIX) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(cannaManDir)/man$(MANSUFFIX)); fi
	@if [ -d $(DESTDIR)$(cannaManDir)/ja/man$(MANSUFFIX) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(cannaManDir)/ja/man$(MANSUFFIX)); fi
	$(INSTALL) -c $(INSTMANFLAGS) server.tmp $(DESTDIR)$(cannaManDir)/man$(MANSUFFIX)/$(TARGET).$(MANSUFFIX)
	$(INSTALL) -c $(INSTMANFLAGS) server.jtmp $(DESTDIR)$(cannaManDir)/ja/man$(MANSUFFIX)/$(TARGET).$(MANSUFFIX)
	$(RM) server.tmp server.jtmp
clean::
	$(RM) server.tmp server.jtmp

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

includes:: $(HEADER)
	@for flag in ${MAKEFLAGS} ''; do \
	case "$$flag" in *=*) ;; *[i]*) set +e;; esac; done; \
	echo "	cd" $(CANNAROOT)/include; cd $(CANNAROOT)/include && \
	for i in $(HEADER); do (set -x; $(RM) $$i; $(LN) ../server/$$i .); done

# ----------------------------------------------------------------------
# common rules for all Makefiles - do not edit

.c.i:
	$(RM) $@
	 $(CC) -E $(CFLAGS) $(_NOOP_) $*.c > $@

emptyrule::

clean::
	$(RM) *.CKP *.ln *.BAK *.bak *.o core errs ,* *~ *.a .emacs_* tags TAGS make.log MakeOut  "#"*

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

man_keywords::

# ----------------------------------------------------------------------
# empty rules for directories that do not have SUBDIRS - do not edit

install::
	@echo "install in $(CURRENT_DIR) done"

install.man::
	@echo "install.man in $(CURRENT_DIR) done"

install.linkkit::
	@echo "install.linkkit in $(CURRENT_DIR) done"

